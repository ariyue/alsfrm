        <attribute>
			<name>id</name>
			<type>String</type>
			<requried>true</requried> <!-- 是否必须 -->
			<description>控件ID</description>
		</attribute>
		<attribute>
			<name>text</name>
			<type>String</type>
			<requried>false</requried> <!-- 是否必须 -->
			<description>文本</description>
		</attribute>
		
		<attribute>
			<name>labelClass</name>
			<type>String</type>
			<requried>false</requried> <!-- 是否必须 -->
			<description>样式</description>
		</attribute>
	</tag>
	
	
	
	
	
