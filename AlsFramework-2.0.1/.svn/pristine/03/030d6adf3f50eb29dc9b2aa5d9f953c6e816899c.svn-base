package com.als.frm.ex;

import java.io.Serializable;

public class DBOpratorException extends DBAccessException implements Serializable{

	private static final long serialVersionUID = 1L;
	public DBOpratorException(){
		super();
	}
	
	public DBOpratorException(String msg){
		super(msg);		
	}
	
	public DBOpratorException(String msg, Throwable cause){
		super(msg, cause);		
	}
	
	public DBOpratorException(String msg, String code){
		super(msg, code);		
	}
	
	public DBOpratorException(String msg, String code, Throwable cause){
		super(msg, code, cause);		
	}
	
	public DBOpratorException(String msg, String code, String userId){
		super(msg, code, userId);		
	}
	
	public DBOpratorException(String msg, String code, String userId, Throwable cause){
		super(msg, code, userId, cause);	
	}
}
